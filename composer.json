{
  "name": "diepxuan/ductn",
  "type": "project",
  "description": "Package for Diepxuan Servers",
  "authors": [
    {
      "name": "Tran Ngoc Duc",
      "email": "ductn@diepxuan.com"
    }
  ],
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "diepxuan/laravel-cloudflare": "dev-main",
    "diepxuan/laravel-magento": "dev-master",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^10.0",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "symfony/process": "^6.3",
    "diepxuan/system": "dev-main"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.7",
    "clue/phar-composer": "^1.4"
  },
  "autoload": {
    "exclude-from-classmap": [
      "vendor\\symfony\\finder\\SplFileInfo.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Symfony\\": "app/Overrides/Symfony/",
      "Illuminate\\": "app/Overrides/Illuminate/",
      "Diepxuan\\": "src/Diepxuan/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "files": []
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "build": [
      "chmod +x bin/build",
      "bin/build"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "diepxuan/laravel-magento": "source",
      "diepxuan/laravel-cloudflare": "source",
      "diepxuan/system": "source",
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "bin": [
    "artisan"
  ],
  "prefer-stable": true
}
