#!/usr/bin/env bash
#!/bin/bash

_BASEDIR="/var/www/base"
_BASHDIR="$_BASEDIR/bash"
_LIBDIR="$_BASEDIR/lib"
_BINDIR="$_BASHDIR/bin"

. $_BASHDIR/libraries.sh
. $_BASHDIR/main.sh

[[ ! "$(whoami)" -eq "ductn" ]] && exit 1

if [[ ! -z ${@+x} ]]; then
    "--$@"
    exit 0
else
    main
    exit 0
fi

exit 0
