#!/usr/bin/env bash
#!/bin/bash

_BASEDIR="/var/www/base"
_BASHDIR="$_BASEDIR/bash"
_LIBDIR="$_BASEDIR/lib"
_BINDIR="$_BASHDIR/bin"

. $_BASHDIR/libraries
. $_BASHDIR/main.sh

if [[ ! "$(whoami)" == "ductn" ]]; then exit 1; fi

if [[ ! -z ${@+x} ]]; then
    "--$@"
    exit 0
else
    main
    exit 0
fi

exit 0
