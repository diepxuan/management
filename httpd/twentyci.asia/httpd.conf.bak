########################################################################
# domain: all
########################################################################
<VirtualHost *:80>
    UseCanonicalName Off
    ServerAlias *.twentyci.asia
    VirtualDocumentRoot /var/www/html/dev/%0/
</VirtualHost>

<VirtualHost *:443>
    UseCanonicalName Off
    ServerAlias *.twentyci.asia
    VirtualDocumentRoot /var/www/html/dev/%0/

    SSLEngine on
    SSLProtocol all -SSLv2 -SSLv3

    SSLCipherSuite ALL:!ADH:!EXPORT56:RC4+RSA:+HIGH:+MEDIUM:+LOW:+SSLv2:+EXP

    SSLCertificateFile /var/www/base/httpd/twentyci.asia/certificate.crt
    SSLCertificateKeyFile /var/www/base/httpd/twentyci.asia/private.key
    SSLCACertificateFile /var/www/base/httpd/twentyci.asia/ca_bundle.crt
</VirtualHost>
